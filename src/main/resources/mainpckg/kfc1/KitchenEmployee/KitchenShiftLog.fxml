<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpckg.kfc1.KitchenEmployee.KitchenShiftLogController">
   <children>
      <Label layoutX="298.0" layoutY="38.0" text="Shift Start and End Time">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="364.0" layoutY="97.0" text="Welcome">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Button layoutX="281.0" layoutY="170.0" mnemonicParsing="false" onAction="#endShiftButtonOnAction" prefHeight="25.0" prefWidth="223.0" text="End Shift" />
      <Button layoutX="281.0" layoutY="133.0" mnemonicParsing="false" onAction="#startShiftButtonOnAction" prefHeight="25.0" prefWidth="223.0" text="Start Shift" />
      <VBox fx:id="shiftTimeVbox" layoutX="134.0" layoutY="225.0" prefHeight="200.0" prefWidth="533.0">
         <children>
            <HBox fx:id="shiftStartHbox" prefHeight="70.0" prefWidth="533.0" spacing="20.0">
               <children>
                  <Label text="Shift Start Time:">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Label>
                  <Label fx:id="startShiftTimeLable" prefHeight="20.0" prefWidth="100.0" text="Label">
                     <font>
                        <Font size="14.0" />
                     </font></Label>
               </children>
            </HBox>
            <HBox fx:id="shiftEndHbox" layoutX="10.0" layoutY="10.0" prefHeight="70.0" prefWidth="533.0" spacing="20.0">
               <children>
                  <Label text="Shift End Time:">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Label>
                  <Label fx:id="endShiftTimeLable" prefHeight="20.0" prefWidth="100.0" text="Label">
                     <font>
                        <Font size="14.0" />
                     </font></Label>
               </children>
            </HBox>
            <HBox fx:id="totalDurationHbox" layoutX="10.0" layoutY="80.0" prefHeight="73.0" prefWidth="533.0" spacing="20.0">
               <children>
                  <Label text="Total Duration:">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Label>
                  <Label fx:id="durationLable" prefHeight="20.0" prefWidth="100.0" text="Label">
                     <font>
                        <Font size="14.0" />
                     </font></Label>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
